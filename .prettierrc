{
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ],
    "pluginSearchDirs": [
        "."
    ],
    "plugins": [
        "prettier-plugin-svelte",
        "prettier-plugin-astro"
    ],
    "printWidth": 100,
    "singleQuote": false,
    "trailingComma": "none",
    "useTabs": true
}